pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building...'
                echo 'Building Done'
            }
        }
        stage("Push") {
            steps {
                echo 'Pushing...'
                echo 'Pushing Done'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying...'
                echo 'Deploying Done'
            }
        }
    }
}
